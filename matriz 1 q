#include <stdio.h>

int main() {
    int matriz[4][4], contador = 0;

    printf("Digite os elementos da matriz 4x4:\n");

    for (int i = 0; i < 4; i++)
        for (int j = 0; j < 4; j++) {
            scanf("%d", &matriz[i][j]);
            contador += (matriz[i][j] > 10);
        }

    printf("%d valores maiores dd que 10.\n", contador);

    return 0;
}
